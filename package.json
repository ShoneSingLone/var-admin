{
  "name": "var-admin",
  "version": "0.0.1",
  "description": "CRM with Koa2, Webpack,Vue Angular React=>var",
  "scripts": {
    "changeversion": "node ./config/pre/changeversion.js",
    "git:pull": "git pull && git pull gitee master && git pull coding master",
    "git:push": "npm run changeversion && git push && git push gitee master && git push coding master",
    "lint": "eslint . --ignore-path .eslintignore",
    "lint:fix": "eslint --fix . --ignore-path .eslintignore",
    "build": "cross-env NODE_ENV=production webpack --progress --profile --config config/index.js --json > stats.json",
    "start": "node server.js",
    "start:s": "cross-env NODE_ENV=development nodemon server.js",
    "serve": "cross-env NODE_ENV=production PORT=10080 pm2 start server.js -i 4 --name koa-webpack-example --merge-logs --log-date-format='YYYY-MM-DD HH:mm Z' && pm2 save 2> /dev/null",
    "vue:s": "vue-cli-service serve",
    "vue:b": "vue-cli-service build",
    "vl": "vue-cli-service build --target lib --name vlibs --dest public/static/vlibs src/index.libs.js "
  },
  "author": "ShoneSingLone",
  "repository": {
    "type": "git",
    "url": "https://github.com/shonesinglone/var-admin.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/preset-env": "^7.9.0",
    "ant-design-vue": "^1.4.10",
    "axios": "^0.19.1",
    "d3": "^5.16.0",
    "element-ui": "^2.13.0",
    "jquery": "^3.4.1",
    "jsstore": "^3.10.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^7.4.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "@babel/core": "^7.10.2",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@vue/cli-service": "^4.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "confman": "^0.2.10",
    "cross-env": "^5.1.5",
    "css-loader": "^0.28.11",
    "eslint-plugin-vue": "^6.2.1",
    "file-loader": "^1.1.11",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "koa": "^2.5.1",
    "koa-mount": "^3.0.0",
    "koa-static": "^4.0.3",
    "less": "^3.11.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-terser": "^5.2.0",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue-loader": "^15.8.3",
    "vue-router": "^3.1.5",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run changeversion"
    }
  }
}
